{
  "name": "cloudschool-api",
  "version": "0.1.0",
  "private": true,
  "description": "cloudschool-api",
  "main": "lib/main.js",
  "scripts": {
    "build": "babel src --ignore \"_*\" --out-dir lib",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "flow",
    "test-src": "jest",
    "start:dev": "nodemon src/main.js --exec babel-node  --presets es2015-native-generators,stage-2",
    "start": "node lib/main.js",
    "test": "npm run lint && npm run typecheck"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "graphql-yoga": "^1.16.1",
    "prisma-binding": "^2.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015-native-generators": "^6.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^5.4.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "flow-bin": "^0.79.1",
    "flow-typed": "^2.5.1",
    "jest": "^23.5.0",
    "nodemon": "^1.14.12",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  }
}
